{
  "type": "object",
  "patternProperties": { "^_[^_]+": { "type": ["number", "string"] } },
  "required": [
    "startedDateTime",
    "id",
    "title",
    "pageTimings"
  ],
  "properties": {
    "startedDateTime": {
      "type": "string",
      "format": "date-time"
    },
    "id": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "_verifications": {
      "type": "array",
      "items": {
        "$ref": "#/components/schemas/VerifyResult"
      },
      "default": []
    },
    "_counters": {
      "type": "array",
      "items": {
        "$ref": "#/components/schemas/Counter"
      },
      "default": []
    },
    "_errors": {
      "type": "array",
      "items": {
        "$ref": "#/components/schemas/Error"
      },
      "default": []
    },
    "pageTimings": {
      "$ref": "#/components/schemas/PageTimings"
    },
    "comment": {
      "type": "string"
    }
  }
}
