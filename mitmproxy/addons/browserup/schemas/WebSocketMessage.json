  "patternProperties": { "^_[^_]+": { "type": ["number", "string"] } },
  "required": [
    "type",
    "opcode",
    "data",
    "time"
  ],
  "properties": {
    "type": {
      "type": "string"
    },
    "opcode": {
      "type": "number"
    },
    "data": {
      "type": "string"
    },
    "time": {
      "type": "number"
    }
  }
}
